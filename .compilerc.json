{
  "env": {
    "development": {
      "application/javascript": {
        "presets": [
          ["env", { "targets": { "electron": "1.6.0" } }],
          "react",
          "stage-1"
        ],
        "plugins": ["transform-async-to-generator",  "react-hot-loader/babel"],
        "sourceMaps": "inline"
      }
    },
    "production": {
      "application/javascript": {
        "presets": [
          ["env", { "targets": { "electron": "1.6.0" } }],
          "react",
          "stage-1"
        ],
        "plugins": ["transform-async-to-generator"],
        "sourceMaps": "none"
      }
    }
  }
}